<configuration name="dnd_control.conf" description="DND and Queue Control">
    <settings>
        <param name="dnd-enabled" value="true"/>
        <param name="queue-pause-enabled" value="true"/>
        <param name="dnd-fallback" value="voicemail"/>
        <param name="dnd-message" value="Extension is in Do Not Disturb mode"/>
    </settings>

    <dnd-settings>
        <extension id="1001">
            <param name="dnd-status" value="off"/>
            <param name="dnd-fallback" value="voicemail"/>
            <param name="queue-pause" value="false"/>
        </extension>

        <extension id="1003">
            <param name="dnd-status" value="off"/>
            <param name="dnd-fallback" value="voicemail"/>
            <param name="queue-pause" value="false"/>
        </extension>

        <extension id="1000">
            <param name="dnd-status" value="off"/>
            <param name="dnd-fallback" value="voicemail"/>
            <param name="queue-pause" value="false"/>
        </extension>

        <extension id="1002">
            <param name="dnd-status" value="off"/>
            <param name="dnd-fallback" value="voicemail"/>
            <param name="queue-pause" value="false"/>
        </extension>

        <extension id="1004">
            <param name="dnd-status" value="off"/>
            <param name="dnd-fallback" value="voicemail"/>
            <param name="queue-pause" value="false"/>
        </extension>

        <extension id="1005">
            <param name="dnd-status" value="off"/>
            <param name="dnd-fallback" value="voicemail"/>
            <param name="queue-pause" value="false"/>
        </extension>
    </dnd-settings>

    <queue-control>
        <agent id="1001@default">
            <param name="pause-status" value="false"/>
            <param name="pause-reason" value=""/>
        </agent>

        <agent id="1003@default">
            <param name="pause-status" value="false"/>
            <param name="pause-reason" value=""/>
        </agent>
    </queue-control>
</configuration>
