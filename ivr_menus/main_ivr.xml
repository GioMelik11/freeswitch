    <menu name="main_ivr" greet-long="/usr/share/freeswitch/sounds/ivr/incoming.wav" greet-short="/usr/share/freeswitch/sounds/ivr/incoming.wav" invalid-sound="/usr/share/freeswitch/sounds/ivr/incoming.wav" exit-sound="/usr/share/freeswitch/sounds/ivr/voicemail/incoming.wav" confirm-macro="" confirm-key="" tts-engine="flite" tts-voice="callie" confirm-attempts="3" timeout="15000" inter-digit-timeout="3000" max-failures="3" max-timeouts="3" digit-len="1">
      <entry action="menu-exec-app" digits="1" param="transfer 251144 XML default"/>
