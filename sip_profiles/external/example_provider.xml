<include>
  <!-- Sample SIP Trunk Configuration -->
  <!-- Replace with your actual SIP provider details -->
  
  <gateway name="example_provider">
    <param name="username" value="your_username"/>
    <param name="realm" value="sip.example.com"/>
    <param name="password" value="your_password"/>
    <param name="proxy" value="sip.example.com"/>
    <param name="register" value="true"/>
    <param name="register-transport" value="udp"/>
    <param name="retry-seconds" value="30"/>
    <param name="expire-seconds" value="300"/>
    <param name="caller-id-in-from" value="true"/>
    <param name="supress-cng" value="true"/>
  </gateway>
  
</include>
